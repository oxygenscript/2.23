window.onload=function(){var ossTags;document.querySelectorAll("oss").forEach(function(tags){var os="off";console.log("compiler is connected");var os1=/os:\ on/,os2=/os:\ off/,osoff=tags.textContent.match(os2),oson=tags.textContent.match(os1);if(osoff){var os="off";console.log("The compiler was disabled")}if(oson)var os="on";if(tags.innerHTML="","on"==os){var osTags=document.querySelectorAll("os");osTags.forEach(function(tag){var run1=/run\.text\("(.*)"\)/,run2=/run\.html\("(.*)"\)/,browser1=/browser\.name()/,browser2=/browser\.version()/,console1=/console\.log\("(.*)"\)/,console2=/console\.error\("(.*)"\)/,calendar1=/calendar\.hours()/,calendar2=/calendar\.day()/,calendar3=/calendar\.year()/,calendar4=/calendar\.month()/,calendar5=/calendar\.minutes()/,calendar6=/calendar\.seconds()/,open1=/open\.url\("(.*)"\)/,math=/math\("(.*)"\)/,help=/help()/,runtext=tag.textContent.match(run1),runhtml=tag.innerHTML.match(run2),browsern=tag.innerHTML.match(browser1),browserv=tag.innerHTML.match(browser2),consolelog=tag.textContent.match(console1),consoleerror=tag.textContent.match(console2),calendarhours=tag.textContent.match(calendar1),calendarminutes=tag.textContent.match(calendar5),calendarseconds=tag.textContent.match(calendar6),calendarday=tag.textContent.match(calendar2),calendaryear=tag.textContent.match(calendar3),calendarmonth=tag.textContent.match(calendar4),openurl=tag.textContent.match(open1),math1=tag.textContent.match(math),help1=tag.textContent.match(help);if(runtext){var text=runtext[1],result=text.substring(0,text.length-0);tag.innerHTML=result}if(runhtml){var text=runhtml[1],result=text.substring(0,text.length-0);tag.innerHTML=result}if(browsern&&(tag.innerHTML=navigator.userAgent),consolelog){var text=consolelog[1],result=text.substring(0,text.length-0);console.log(result),tag.innerHTML=""}if(consoleerror){var text=consoleerror[1],result=text.substring(0,text.length-0);console.error(result),tag.innerHTML=""}if(browserv&&(tag.innerHTML=navigator.appVersion),calendarhours){var currentDate=new Date;tag.innerHTML=currentDate.getHours()}if(calendarday){var currentDate=new Date;tag.innerHTML=currentDate.getDate()}if(calendaryear){var currentDate=new Date;tag.innerHTML=currentDate.getFullYear()}if(calendaryear){var currentDate=new Date;tag.innerHTML=currentDate.getMonth()+1}if(calendarminutes){var currentDate=new Date;tag.innerHTML=currentDate.getMinutes()}if(calendarseconds){var currentDate=new Date;tag.innerHTML=currentDate.getSeconds()}if(openurl){var text=openurl[1],result=text.substring(0,text.length-0);window.open(result),tag.innerHTML=""}if(math1){var text=math1[1],result=text.substring(0,text.length-0);try{tag.innerHTML=eval(result)}catch(e){console.error("Error in math transaction"),tag.innerHTML=""}}if(help1){var text='Oxygen script 2.23<br>Made by Abolfazl Jamali in Iran<br><br>OSS tag:<br><br>Compiler settings:<br>os: off  : Disable the compiler<br>os: on  : Enable the compiler<br><br><br>OS tag:<br><br>RUN codes:<br>run.text("your text") : Writing text<br>run.html("your html code") : Write html code<br> <br>BROWSER codes:<br>browser.name() : Write the name of the browser<br>browser.version() : Writing a browser version<br><br>CONSOLE codes:<br>console.log("your text") : Write a report to the console<br>console.error("your text") : Write an error to the console<br><br>CALENDAR codes:<br>calendar.hours() : writing hoursr<br>calendar.minutes() : Write minutes<br>calendar.seconds() : Write seconds<br>calendar.day() : Writing of the day<br>calendar.month() : Write the month<br>calendar.year() : Write the year<br><br>OPEN code:<br>open.url("your link") : Open the link<br><br>MATH code:<br>math("2+3") : Performing mathematical operations';tag.innerHTML=text}})}else{var osTags=document.querySelectorAll("os");osTags.forEach(function(r){r.innerHTML=""})}})};
